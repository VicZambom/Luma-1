{
    "extends": [
        "@rocketseat/eslint-config/react",
        "react-app",
        "react-app/jest",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "react/jsx-indent": ["error", 2],
        "react/jsx-indent-props": ["error", 2],
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}